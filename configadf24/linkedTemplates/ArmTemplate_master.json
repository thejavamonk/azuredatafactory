{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "configadf24"
		},
		"ls_azure_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql'"
		},
		"ls_adls_configadf24_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sadev2024.dfs.core.windows.net/"
		},
		"ls_azure_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devsqlserver202408.database.windows.net"
		},
		"ls_azure_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dev-db"
		},
		"ls_azure_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"ls_dev_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dev-akv-202408.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "configadf24_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_sql_password": {
						"value": "[parameters('ls_azure_sql_password')]"
					},
					"ls_adls_configadf24_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_configadf24_properties_typeProperties_url')]"
					},
					"ls_azure_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_server')]"
					},
					"ls_azure_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_database')]"
					},
					"ls_azure_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_userName')]"
					},
					"ls_dev_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_dev_akv_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}